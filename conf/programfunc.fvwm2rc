#SetEnv back_borderwidth 1
#SetEnv front_borderwidth 8
#SetEnv fvwmbuttons_width 60
#SetEnv fvwmpager_width 144
#SetEnv fvwmiconman_height 28

DestroyFunc CurrentCenter
AddToFunc CurrentCenter
+ I Next Focus
+ I Current Piperead 'echo Resize\
	$(($[vp.width]\
	-$[fvwm_left_margin]\
	-$[fvwm_right_margin]\
	-2*$[front_borderwidth]))p \
 	$(($[vp.height]\
	-2*($[fvwmiconman_height]\
	+2*$[back_borderwidth]\
	+$[front_borderwidth])))p'
+ I Current AnimatedMove 50-50w 50-50w

DestroyFunc FvwmXTerm
AddToFunc   FvwmXTerm
+ I Exec exec $[fvwm_xterm]
+ I Wait Xterm
+ I CurrentCenter

DestroyFunc FvwmGTerm
AddToFunc   FvwmGTerm
+ I Exec exec $[fvwm_gnome-terminal] --hide-menubar
+ I Wait Gnome-terminal
+ I CurrentCenter

DestroyFunc FvwmChrome
AddToFunc   FvwmChrome
